@Html.Action("city", new { province = CookieHelper.Province })

<nav class="top-menu">
    <div class="top-menu-tab">
        <div class="top-menu-category">
            <h2>
                <strong>全部户外分类</strong>
            </h2>
        </div>
        <ul class="top-menu-list">
            <li class="active"><a href="@Url.Action("index", "home", new { province = CookieHelper.Province })">首页</a></li>
            <li><a href="@Url.Action("index", "list", new
                    {
                        province = CookieHelper.Province,
                        city = 0,
                        mediacode = 0,
                        childmediacode = 0,
                        formatcode = 0,
                        ownercode = 0,
                        periodcode = 0,
                        page = 1
                    })">户外资源</a></li>
