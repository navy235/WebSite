        @Html.Partial("_LeftMenu", (List<HttpLinkGroup>)ViewBag.LeftMenu)
    </div>
    <div class="top-hot-right cleafix">
        @Html.Partial("_SliderBox", (List<HttpLinkItem>)ViewBag.SliderBox)

